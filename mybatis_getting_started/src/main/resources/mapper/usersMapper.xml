<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace 命名空间：通常情况，命名空间的值就是当前操作实体类的全名称 -->
<mapper namespace="com.ishavanti.entity.Users">
    <select id="usersList" resultType="com.ishavanti.entity.Users">
        select * from users;
    </select>
</mapper>